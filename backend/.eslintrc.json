{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": "off",
        "function-paren-newline": "off",
        "global-require": "off",
        "implicit-arrow-linebreak": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}
